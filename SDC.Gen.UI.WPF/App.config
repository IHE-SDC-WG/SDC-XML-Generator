      <setting name="BrowserPath64" serializeAs="String">
        <value>C:\Program Files (x86)\Mozilla Firefox\firefox.exe</value>
      </setting>
      <setting name="BrowserPath32" serializeAs="String">
        <value>C:\Program Files\Mozilla Firefox\firefox.exe</value>
      </setting>
      <setting name="DataSourceConnectionString" serializeAs="String">
