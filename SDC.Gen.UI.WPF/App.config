  <connectionStrings>
    <add name="SSPEntities" 
         connectionString="metadata=res://*/Model1.csdl|res://*/Model1.ssdl|res://*/Model1.msl;provider=System.Data.SqlClient;provider connection string='data source=sdtPRODdb.database.windows.net;initial catalog=SSPPROD;authentication=&quot;Active Directory Integrated&quot;;MultipleActiveResultSets=True;App=EntityFramework'"
